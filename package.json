{
  "name": "functer",
  "version": "0.0.0-development",
  "description": "Functors in TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/*.ts",
    "test": "jest --config jestconfig.json",
    "test:cov": "npm test -- --collectCoverage=true",
    "prepublishOnly": "npm test && npm run lint",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/futantan/functer.git"
  },
  "keywords": [
    "functor"
  ],
  "author": "futantan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/futantan/functer/issues"
  },
  "homepage": "https://github.com/futantan/functer#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/ramda": "github:types/npm-ramda#dist",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "codecov": "^3.6.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ramda": "^0.26.1",
    "semantic-release": "^17.2.3",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
